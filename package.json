{
  "name": "medsim-innovations",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "docker:up": "docker-compose up",
    "docker:build-and-up": "npm run docker:down && docker-compose up --build",
    "docker:down": "npm run docker:down-containers && npm run docker:down-images && npm run docker:down-volumes && npm run docker:down-networks",
    "docker:down-containers": "docker ps -aq --filter 'name=medsim-innovations' | xargs docker rm",
    "docker:down-images": "docker images -q --filter 'reference=medsim-innovations' | xargs docker rmi",
    "docker:down-volumes": "docker volume ls -qf dangling=true | xargs docker volume rm",
    "docker:down-networks": "docker network ls -q --filter 'name=medsim-innovations' | xargs docker network rm"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@libsql/client": "^0.14.0",
    "@tabler/icons-react": "^3.31.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.41.0",
    "esbuild": "^0.25.1",
    "framer-motion": "^11.17.0",
    "jose": "^5.10.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.477.0",
    "next": "15.2.3",
    "nodemailer": "^6.9.16",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.6.0",
    "uuid": "^11.0.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "drizzle-kit": "^0.30.1",
    "eslint": "^8",
    "eslint-config-next": "15.0.1",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
